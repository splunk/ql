[verify_certs]
value = <boolean>
* Determines whether or not to check for a valid certificate when making calls to SOAR
* Default: true

[log_level]
value = <string>
* Set log level
* This value will be used throughout the app,
  including mod input
* Available options: "DEBUG", "INFO", "WARNING", "ERROR", "CRITICAL"
* Default: "INFO"

[version]
value = <string>
* Version of this app
