# Application-level permissions
#### SOAR Dashboards ####
## Note: phantomdelete is used for remote search
[]
access = read : [ * ], write : [ admin, phantomdelete, power ]
export = system

#### SOAR Remote Search ####
[datamodels/Phantom_Remote_Search]
access = read : [ * ], write : [ admin, phantomdelete ]
export = system

### EVENT TYPES
[eventtypes]
export = system

### PROPS
[props]
export = system

### TRANSFORMS
[transforms]
export = system

### VIEWSTATES: even normal users should be able to create shared viewstates
[viewstates]
access = read : [ * ], write : [ * ]
export = system

#### Splunk App for SOAR - used for ITSI integration ####
[views]
access = read : [ * ], write : [ admin ]
export = none
